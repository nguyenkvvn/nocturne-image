/dev/system_a   /           ext4     rw,noload,noauto,noatime,errors=remount-ro  0 1
/dev/system_b   /           ext4     rw,noload,noauto,noatime,errors=remount-ro  0 1
/dev/data       /var        ext4     rw                  0 0
/dev/settings   /var/lib    ext4     rw                  0 0

/var/local/root /root       none     defaults,bind       0 0

/var/swapfile   none        swap     defaults            0 0

proc            /proc       proc     defaults            0 0
sysfs           /sys        sysfs    defaults            0 0
devpts          /dev/pts    devpts   gid=4,mode=620      0 0
tmpfs           /dev/shm    tmpfs    defaults            0 0
tmpfs           /tmp        tmpfs    defaults            0 0
tmpfs           /run        tmpfs    defaults            0 0